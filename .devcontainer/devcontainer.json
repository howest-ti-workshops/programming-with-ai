{
	"name": "Deno",
	"build": { "dockerfile": "Dockerfile" },
	"hostRequirements": { "cpus": 2},
	"customizations": {
		"vscode": {
			"extensions": [
				"denoland.vscode-deno"
			]
		}
		,
		"settings": { 
			"deno.enable": true,
			"deno.lint": true,
			"editor.defaultFormatter": "denoland.vscode-deno"
		}
	}
}